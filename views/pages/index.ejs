      <div class="container-fluid cards">
        <div class="row">
          <div class="col-md-4">
            <div class="card">
              <img class="card-img-top" src="./images/image1.jpg" alt="Card image cap">
              <div class="card-body">
                <h5 class="card-title user">Faculty Publications</h5>
                <a href="/userReview" class=" btn2 btn btn-primary stretched-link ml-auto">Add Publications</a>
              </div>
            </div>
          </div>
          <div class="col-md-4">
            <div class="card">
              <img class="card-img-top" src="./images/image2.jpg" alt="Card image cap">
