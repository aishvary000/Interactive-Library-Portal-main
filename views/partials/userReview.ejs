<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/headers.ejs') %>
</head>
<body>
  <%- include('../partials/navbar.ejs',{User: User,LoggedIn:LoggedIn}) %>
    <form class>
      <div class="row">
        
          <div class="row justify-content-center">
            <div class="col-md-12">
              <span class="anchor" id="formUserEdit"></span>
              
              <!-- form user info -->
              <div class="card card-outline-secondary">
                <div class="card-header text-center">
                  <h3 class="mb-0">User Review</h3>
                </div>
                <div class="card-body">
                  <form autocomplete="off" class="form" role="form">
                    <div class="form-group row">
                      <label class="col-lg-3 col-form-label form-control-label">First name</label>
                      <div class="col-lg-9">
                        <input class="form-control" type="text" value="First Name">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label class="col-lg-3 col-form-label form-control-label">Last name</label>
                      <div class="col-lg-9">
                        <input class="form-control" type="text" value="Last Name">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label class="col-lg-3 col-form-label form-control-label">Email</label>
                      <div class="col-lg-9">
                        <input class="form-control" type="email" value="Enter your Email">
                      </div>
                    </div>
                    
                    <div class="form-group row">
                      <label class="col-lg-3 col-form-label form-control-label">Give your Feedback</label>
                      <div class="col-lg-9">
                        <textarea class="form-control" id="message2" name="message2" required="" rows="6"></textarea>
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="exampleFormControlFile1" class="col-lg-3 col-form-label form-control-label" >Example file input</label>
                      <input type="file" class="form-control-file" id="exampleFormControlFile1">
                    </div>
                    <div class="form-group row">
                      <label class="col-lg-3 col-form-label form-control-label"></label>
                      <div class="col-lg-9">
                        <a href="#" class="btn3 btn btn-primary" >Save Changes</a>
                        <a href="#" class="btn3 btn btn-secondary" >Cancel</a>

                      </div>
                    </div>
                  </form>
                </div>
              </div><!-- /form user info -->
            </div>
          </div>
      
</body>